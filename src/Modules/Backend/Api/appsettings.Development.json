{
  "ConnectionStrings": {
    "Database": "Host=stamina-warrior-db;Port=5432;Database=StaminaWarrior;Username=postgres;Password=postgres;"
  },
  "Outbox": {
    "JobProcessingInterval": 20,
    "BatchSize": 20,
    "RetryCount": 3
